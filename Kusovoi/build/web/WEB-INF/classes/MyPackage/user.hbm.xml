<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="MyPackage.User" table="kursovoi">
        <id column="IdUser" name="id" type="java.lang.Integer" />
        <property column="NickName" name="userName" type="java.lang.String" />
